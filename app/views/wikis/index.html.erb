<h1>Wikis</h1>

 <div class="row">
   <div class="col-md-8">
     <% if current_user %>
     <div class="col-md-4">
       <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
     </div>
     <% end %><br /><br />
     <% @wikis.each do | wiki | %>
       <div class="media">
         <div class="media-body">
           <h4 class="media-heading">
             <%= link_to wiki.title, wiki %>
             <h5><%= wiki.body %>
               <small>
               <%= link_to "Delete", wiki, method: :delete, data: {confirm: 'Are you sure you want to delete this Wiki?'} %>
             </small>
           </h5>
          </h4>
         </div>
       </div>
       <% end %>
   </div>
 </div>
